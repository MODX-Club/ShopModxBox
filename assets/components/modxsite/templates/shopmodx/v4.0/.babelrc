{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-es3-property-literals",
    "transform-es3-member-expression-literals",
    ["module-resolver", {
      "root": ["./app"],
      "alias": {
        "modules": "./app/modules"
      }
    }]
  ]
}